{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noImplicitAny": false,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "skipLibCheck": true,
    "strict": false,
    "strictFunctionTypes": false,
    "strictNullChecks": false
  },
  "exclude": [
    "node_modules",
    "dist",
    "developer-friendly-blog",
    "social-media-presence",
    "ignore",
    "ralph-docs"
  ],
  "extends": "astro/tsconfigs/base"
}
